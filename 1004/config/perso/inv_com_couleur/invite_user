# Mon invite de commande utilisateur
# user_install
MARRON='\[\033[0;33m\]'
JAUNE='\[\033[1;33m\]'
BLANC='\[\033[1;37m\]'
NC='\[\033[0m\]'
PS1="$MARRON[$JAUNE\u$NC@$JAUNE\h$NC:$BLANC\w$MARRON] $BLANC\$ $NC"

# Alias de base
alias ll='ls -lh'
alias aa='sudo apt-get install'
alias uu='sudo apt-get update && apt-get dist-upgrade'

